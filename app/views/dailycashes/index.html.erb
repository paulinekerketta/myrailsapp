<p id="notice"><%= notice %></p>

<h1>Dailycashes</h1>
<div class="media">
<div class="media-body">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Sales</th>
      <th>Cashpaid</th>
      <th>Onlinepaid</th>
      <th>Driverpaid</th>
      <th>Banklodge</th>
      <th>Overunder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailycashes.each do |dailycash| %>
      <tr>
        <td><%= dailycash.date.strftime("%d/%m/%Y") %></td>
        <td><%= dailycash.sales %></td>
        <td><%= dailycash.cashpaid %></td>
        <td><%= dailycash.onlinepaid %></td>
        <td><%= dailycash.driverpaid %></td>
        <td><%= dailycash.banklodge %></td>
        <td><%= dailycash.overunder %></td>
        <td><%= link_to 'Show', dailycash, class:'btn btn-mini btn-default' %></td>
        <td><%= link_to 'Edit', edit_dailycash_path(dailycash), class:'btn btn-mini btn-success' %></td>
        <td><%= link_to 'Destroy', dailycash, method: :delete, class:'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  </div>
  </div>
</table>

<br>

<%= link_to 'New Dailycash', new_dailycash_path %>
